

#original kyber768 speed test file compilation
gcc -Wall -Wextra -Wpedantic -Wmissing-prototypes -Wredundant-decls -Wshadow -Wpointer-arith -mavx2 -mbmi2 -mpopcnt -maes -march=native -mtune=native -O3 -fomit-frame-pointer -DKYBER_K=3 kem.c indcpa.c polyvec.c poly.c fq.S shuffle.S ntt.S invntt.S basemul.S consts.c rejsample.c cbd.c verify.c fips202.c fips202x4.c symmetric-shake.c keccak4x/KeccakP-1600-times4-SIMD256.o cpucycles.c speed_print.c kex.c test_speed.c -o test_speed768  -lmceliece348864avx -lkeccak -lcrypto -ldl -lsodium

#original kyber1024 speed test file compilation
gcc -Wall -Wextra -Wpedantic -Wmissing-prototypes -Wredundant-decls -Wshadow -Wpointer-arith -mavx2 -mbmi2 -mpopcnt -maes -march=native -mtune=native -O3 -fomit-frame-pointer -DKYBER_K=4 kem.c indcpa.c polyvec.c poly.c fq.S shuffle.S ntt.S invntt.S basemul.S consts.c rejsample.c cbd.c verify.c fips202.c fips202x4.c symmetric-shake.c keccak4x/KeccakP-1600-times4-SIMD256.o cpucycles.c speed_print.c kex.c test_speed.c -o test_speed1024 -lmceliece348864avx -lkeccak -lcrypto -ldl -lsodium


#kyber768 with backdoor speed test file compilation
gcc -Wall -Wextra -Wpedantic -Wmissing-prototypes -Wredundant-decls -Wshadow -Wpointer-arith -mavx2 -mbmi2 -mpopcnt -maes -march=native -mtune=native -O3 -fomit-frame-pointer -DKYBER_K=3 kem.c indcpabackdoor.c polyvec.c poly.c fq.S shuffle.S ntt.S invntt.S basemul.S consts.c rejsample.c cbd.c verify.c fips202.c fips202x4.c symmetric-shake.c keccak4x/KeccakP-1600-times4-SIMD256.o cpucycles.c speed_print.c kex.c test_backdoor_speed.c -o test_backdoor_speed768  -lmceliece348864avx -lkeccak -lcrypto -ldl -lsodium 

#kyber1024 with backdoor speed test file compilation
gcc -Wall -Wextra -Wpedantic -Wmissing-prototypes -Wredundant-decls -Wshadow -Wpointer-arith -mavx2 -mbmi2 -mpopcnt -maes -march=native -mtune=native -O3 -fomit-frame-pointer -DKYBER_K=4 kem.c indcpabackdoor.c polyvec.c poly.c fq.S shuffle.S ntt.S invntt.S basemul.S consts.c rejsample.c cbd.c verify.c fips202.c fips202x4.c symmetric-shake.c keccak4x/KeccakP-1600-times4-SIMD256.o cpucycles.c speed_print.c kex.c test_backdoor_speed.c -o test_backdoor_speed1024  -lmceliece348864avx -lkeccak -lcrypto -ldl -lsodium